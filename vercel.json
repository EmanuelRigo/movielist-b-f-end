{
  "version": 2,
  "builds": [
    {
      "src": "api/src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/app/**/*.tsx",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/app/$1"
    }
  ]
}